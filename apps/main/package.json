{
	"name": "main",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview && wrangler dev",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"db:push": "drizzle-kit push",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:studio": "drizzle-kit studio",
		"graphql:resolvers:gen": "graphql-codegen --config ./graphql.config.server.ts",
		"cf:gen": "wrangler types ./src/worker-configuration.d.ts",
		"gen": "pnpm graphql:resolvers:gen && pnpm cf:gen",
		"predev": "pnpm gen",
		"prebuild": "pnpm gen"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@graphql-codegen/add": "^5.0.3",
		"@graphql-codegen/cli": "^5.0.7",
		"@graphql-codegen/typescript": "^4.1.6",
		"@graphql-codegen/typescript-resolvers": "^4.5.1",
		"@ianvs/prettier-plugin-sort-imports": "^4.4.1",
		"@lucide/svelte": "^0.525.0",
		"@sveltejs/adapter-cloudflare": "^7.0.4",
		"@sveltejs/kit": "<=2.21.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.0.0",
		"@types/node": "^22",
		"daisyui": "^5.0.46",
		"drizzle-kit": "^0.30.6",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"graphql-config": "^5.1.5",
		"houdini": "2.0.0-next.5",
		"houdini-svelte": "3.0.0-next.8",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"simple-icons": "^14.14.0",
		"svelte": "^5.36.13",
		"svelte-check": "^4.0.0",
		"svelte2tsx": "^0.7.39",
		"sveltekit-superforms": "^2.25.0",
		"tailwindcss": "^4.0.0",
		"turbo": "^2.5.5",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"valibot": "^1.1.0",
		"vite": "^6.2.6",
		"vite-plugin-kit-routes": "^0.8.4",
		"vite-plugin-watch-and-run": "^1.7.5",
		"wrangler": "^4.22.0"
	},
	"dependencies": {
		"@escape.tech/graphql-armor": "^3.1.6",
		"@graphql-yoga/plugin-disable-introspection": "^2.16.1",
		"@inlang/paraglide-js": "^2.0.0",
		"better-auth": "^1.3.4",
		"dataloader": "^2.2.3",
		"date-fns": "^4.1.0",
		"drizzle-orm": "^0.40.1",
		"graphql": "^16.11.0",
		"graphql-scalars": "^1.24.2",
		"graphql-yoga": "^5.15.1",
		"js-base64": "^3.7.7",
		"runed": "^0.29.1",
		"stripe": "^18.3.0"
	}
}
